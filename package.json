  "license": "MIT",
  "files": [
    "module",
    "util",
    "plugins"
  ],
  "dependencies": {
    "axios": "^0.20.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "pac-proxy-agent": "^4.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "eslint": "7.7.0",
    "eslint-plugin-html": "6.0.3",
    "husky": "4.2.5",
    "intelli-espower-loader": "1.0.1",
    "lint-staged": "10.2.13",
    "mocha": "8.1.1",
    "power-assert": "1.6.1"
  }
