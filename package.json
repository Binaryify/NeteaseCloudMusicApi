    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-html": "6.0.3",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "intelli-espower-loader": "1.0.1",
